package edu.utexas.yh7483.tankwar;

import java.awt.*;

public class Aid {
	public static final Color AID_COLOR = Color.MAGENTA;
	
	int xpos, ypos, width, height;
	TankClient tc = null;
	
	public Aid(int xpos, int ypos, int width, int height, TankClient tc) {
		super();
		this.xpos = xpos;
		this.ypos = ypos;
		this.width = width;
		this.height = height;
		this.tc = tc;
	}
	
	
}
